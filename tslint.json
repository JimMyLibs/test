{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "src/resource/**",
      "src/**/*.vue",
      "node_modules/**"
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "allow-escape", "avoid-template"],
    "import-name": false,
    "ordered-imports": false,
    "member-ordering": false,
    "ban-types": false,
    "no-string-literal": false,
    "no-console": false,
    "no-empty": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "interface-over-type-literal": false,
    "eofline": false
  },
  "rulesDirectory": []
}