{
    "defaultSeverity": "warning",
    "extends": "tslint:recommended",
    "linterOptions": {
      "exclude": [
        "node_modules/**"
      ]
    },
    "globals": {
      "require": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "globalReturn": true,
        "impliedStrict": true,
        "jsx": false
      },
      "sourceType": "module"
    },
    "rules": {
      "no-trailing-whitespace": false,
      "quotemark": false,
      "semicolon": false,
      "member-access": false,
      "trailing-comma": false,
      "space-before-function-paren": false,
      "no-consecutive-blank-lines": false,
      "no-implicit-dependencies": [true, ["@","@vue","comm", "config", "lrz", "vue-awesome-swiper"]],
      "no-submodule-imports": [true, "@","@vue","comm", "config", "lrz", "vue-awesome-swiper"],
      "indent": [true, "spaces", 4],
      "interface-name": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "no-console": false,
      "no-debugger": false,
      "no-unused-vars": false,
      "linebreak-style": false
    }
  }